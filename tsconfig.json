{
  "compileOnSave": true,
  "compilerOptions": {
    "strictNullChecks": false,
    "allowUnreachableCode": false,
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "target": "es2015",
    "types": ["googlemaps"],
    "module": "esnext",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2018", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@gosi-ui/core": ["libs/core/src/index.ts"],
      "@gosi-ui/core/*": ["libs/core/src/*"],
      "@gosi-ui/foundation-theme": ["libs/foundation/theme/src/index.ts"],
      "@gosi-ui/foundation-theme/*": ["libs/foundation/theme/src/*"],
      "@gosi-ui/foundation-dashboard": ["libs/foundation/dashboard/src/index.ts"],
      "@gosi-ui/foundation-dashboard/*": ["libs/foundation/dashboard/src/*"],
      "@gosi-ui/foundation/inbox": ["libs/foundation/inbox/src/index.ts"],
      "@gosi-ui/foundation/inbox/*": ["libs/foundation/inbox/src/*"],
      "@gosi-ui/foundation/form-fragments": ["libs/foundation/form-fragments/src/index.ts"],
      "@gosi-ui/foundation/form-fragments/*": ["libs/foundation/form-fragments/src/*"],
      "@gosi-ui/features/collection/billing": ["libs/features/collection/billing/src/index.ts"],
      "@gosi-ui/features/collection/billing/*": ["libs/features/collection/billing/src/*"],
      "@gosi-ui/features/contributor": ["libs/features/contributor/src/index.ts"],
      "@gosi-ui/features/contributor/*": ["libs/features/contributor/src/*"],
      "@gosi-ui/features/customer-information": ["libs/features/customer-information/src/index.ts"],
      "@gosi-ui/features/customer-information/*": ["libs/features/customer-information/src/*"],
      "@gosi-ui/features/establishment": ["libs/features/establishment/src/index.ts"],
      "@gosi-ui/features/establishment/*": ["libs/features/establishment/src/*"],
      "@gosi-ui/features/occupational-hazard": ["libs/features/occupational-hazard/src/index.ts"],
      "@gosi-ui/features/occupational-hazard/*": ["libs/features/occupational-hazard/src/*"],
      "@gosi-ui/features/benefits/*": ["libs/features/benefits/src/*"],
      "@gosi-ui/features/benefits": ["libs/features/benefits/src/index.ts"],
      "@gosi-ui/features/payment/*": ["libs/features/payment/src/*"],
      "@gosi-ui/features/payment": ["libs/features/payment/src/index.ts"],
      "@gosi-ui/features/violations": ["libs/features/violations/src/index.ts"],
      "@gosi-ui/features/violations/*": ["libs/features/violations/src/*"],
      "@gosi-ui/foundation/transaction-tracing": ["libs/foundation/transaction-tracing/src/index.ts"],
      "@gosi-ui/foundation/transaction-tracing/*": ["libs/foundation/transaction-tracing/src/*"],
      "@gosi-ui/features/team-management": ["libs/features/team-management/src/index.ts"],
      "@gosi-ui/features/team-management/*": ["libs/features/team-management/src/*"],
      "@gosi-ui/features/medical-board": ["libs/features/medical-board/src/index.ts"],
      "@gosi-ui/features/medical-board/*": ["libs/features/medical-board/src/*"],
      "@gosi-ui/features/complaints": ["libs/features/complaints/src/index.ts"],
      "@gosi-ui/features/complaints/*": ["libs/features/complaints/src/*"],
      "@gosi-ui/features/feature360": ["libs/features/feature360/src/index.ts"],
      "@gosi-ui/features/feature360/*": ["libs/features/feature360/src/*"],
      "@gosi-ui/features/customer-survey": ["libs/features/customer-survey/src/index.ts"],
      "@gosi-ui/features/customer-survey/*": ["libs/features/customer-survey/src/*"],
      "@gosi-ui/features/appeals": ["libs/features/appeals/src/index.ts"],
      "@gosi-ui/features/appeals/*": ["libs/features/appeals/src/*"]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  },
  "exclude": ["node_modules", "tmp", "testing"]
}
